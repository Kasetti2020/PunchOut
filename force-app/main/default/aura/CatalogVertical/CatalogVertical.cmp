                    <aura:iteration items="{!v.completeWrap}" var="prod" indexVar='ind'>
                        <aura:iteration items="{!prod.tempMap}" var="tempValue" indexVar="key">
                            <aura:if isTrue="{!tempValue.key == prod.selectedColor}">
