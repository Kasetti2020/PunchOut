<aura:component controller="VDataUploadctrlCloneNew" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    
    <aura:attribute name="fileName" type="String" default="" />
    <aura:attribute name="footer" type="boolean" default="false"/>
    <aura:attribute name="Next" type="boolean" default="false"/>
    <aura:attribute name="Success" type="boolean" default="false"/>
    <aura:attribute name="RetailNamesNProdVertical" type="List"/>
    <aura:attribute name="VaId" type="Id"/>
    <aura:attribute name="filesize" type="string"/>
    <aura:attribute name="Polifilesize" type="string"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <lightning:spinner aura:id="spinner" class="slds-hide" alternativeText="upload file" style = "position:fixed"/>
    <aura:attribute name="RetailProdVertical" type="List"/>
    <aura:attribute name="RetailTemplate" type="List"/>
    <aura:attribute name="selectedTemplate" type="String"/>
    <aura:attribute name="createContactFlag" type="Boolean" default="false"/>
    <aura:attribute name="UploadfileSize" type="Integer"/>
    <aura:attribute name="MappingSize" type="Decimal"/>
    <aura:attribute name="finalListToAdd" type="list" />
    
    
    <div class="content slds-box slds-p-around_none">
        <header class="slds-modal__header">
            <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse" title="{!$Label.c.Close}">
                
                <span class="slds-assistive-text">{!$Label.c.Close}</span>
            </button>
            <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate" style="margin-left:-24px;">
                <img src="{!$Resource.MagNETIcons + '/Upload.png'}" width="50"/>
                {!$Label.c.PURCHASE_ORDER}</h2>
        </header>
        <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
            <lightning:select aura:id="ProductVertical" name="select2" value="{!v.RetailNamesNProdVertical.ProdVertical}" onchange="{!c.PickTemplateName}" label="{!$Label.c.Select_Vertical}" required="true" class="slds-m-around_x-small">
                <option value="None">{!$Label.c.None}</option>
                <aura:iteration aura:id="TemplateNames" items="{!v.RetailProdVertical}" var="FValues">
                    <option value="{!FValues.fieldAPI}">{!FValues.fieldLabel}</option>
                </aura:iteration>
            </lightning:select>
            <lightning:select aura:id="ProductVertical" name="select2" value="{!v.RetailNamesNProdVertical.Tempalte}" label="{!$Label.c.Select_Template}" required="true" class="slds-m-around_x-small" disabled="{!v.RetailNamesNProdVertical.ProdVertical == 'None' || v.RetailTemplate.length == 0}">
                <option value="None">{!$Label.c.None}</option>
                <aura:iteration aura:id="TemplateNames" items="{!v.RetailTemplate}" var="FetchValues">
                    <option value="{!FetchValues}">{!FetchValues}</option>
                </aura:iteration>
            </lightning:select>
            
            <div class="slds-file-selector slds-file-selector_files slds-m-around_x-small" style="display:block;">
                <label class="slds-form-element__label" for="form-element-01">{!$Label.c.Select_File_to_Upload}</label>                
                <div class="slds-file-selector__dropzone">
                    <input type="file" aura:id="file" onchange="{!c.handleFilesChange}" class="slds-file-selector__input slds-assistive-text" accept=".csv" id="file1" aria-labelledby="file-selector-primary-label file-selector-secondary-label" />  
                    <!-- onchange="{!c.handleFilesChange}"-->
                    
                    <label class="slds-file-selector__body" for="file1" id="file-selector-secondary-label">
                        <span class="slds-file-selector__button slds-button slds-button_neutral" style="padding:16px 18px">
                            <lightning:icon class="slds-button__icon_left" iconName="utility:upload" size="xx-small" alternativeText="{!$Label.c.Indicates_approval}"/>
                            {!$Label.c.Click_here_to_upload_file}
                        </span> 
                        <!-- <span class="slds-file-selector__text slds-medium-show">or Drop Files</span>-->
                    </label>  
                    <div class="slds-text-body_small slds-text-color_error">{!v.fileName} </div>
                    
                </div>
                
                
            </div>
        </div>
        <footer class="slds-modal__footer">
            <!-- <button class="slds-button slds-button_brand" onclick="{!c.viewTemplate}">View Template</button>-->
            <button class="slds-button slds-button_brand" onclick="{!c.downloadTemplate}" disabled="{!v.RetailNamesNProdVertical.Tempalte == 'None' || v.RetailTemplate.length == 0}">	{!$Label.c.Download_Template}</button>
            <button class="slds-button slds-button_brand" onclick="{!c.Uploadfile }">{!$Label.c.Upload}</button>
        </footer>
    </div>
    <aura:if isTrue="{!v.Next}"> 
        <div>
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container" style="width:38%;">
                    <header class="slds-modal__header">
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate confirm-icon">
                            <lightning:icon iconName="utility:multi_select_checkbox" alternativeText="{!$Label.c.Approved}" size="x-small"/> {!$Label.c.Confirm_Purchase_Order}
                        </h2>
                    </header>
                    <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                        <p class="slds-m-bottom_x-small"><span style="width:190px;display:inline-flex;"> {!$Label.c.Number_of_records} </span><span class="slds-text-body_small slds-text-color_error"><b>{!v.Polifilesize}</b></span></p>
                        <p class="slds-m-bottom_large"><span style="width:190px;display:inline-flex;"> {!$Label.c.Number_of_purchase_orders}</span><span class="slds-text-body_small slds-text-color_error"><b>{!v.filesize}</b></span></p>
                        
                        <p>{!$Label.c.Are_you_sure_you_want_to_continue}</p>
                    </div>
                    <footer class="slds-modal__footer">
                        <button class="slds-button slds-button_neutral" onclick="{!c.cancel}">{!$Label.c.Cancel}</button>   
                        <button class="slds-button slds-button_brand" onclick="{!c.SavePO }">{!$Label.c.Confirm}</button>
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </div>	
    </aura:if>
    
    <aura:if isTrue="{!v.Success}"> 
        <div>
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
                <div class="slds-modal__container" style="width:38%;">
                    <header class="slds-modal__header">
