                    </div>
                    <footer class="slds-modal__footer">
                        <button class="slds-button slds-button_brand" style="background:green;color:white;" onclick="{!c.ok}">{!$Label.c.OK}</button> 
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </div>	
    </aura:if>
